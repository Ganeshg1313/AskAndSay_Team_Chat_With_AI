{
    "version": 1,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      },
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "^/$", "dest": "server.js" },
      { "src": "^/(.+)", "dest": "app.js" }
    ]
  }
  